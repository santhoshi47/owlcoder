class Solution {

    void printTriangle(int n) {
        for(int i=1;i<=2*n;i++)
        {
            int star=i<=n?n-i+1:i-n;
            stars(star);
            int space=i<=n?2*i-2:4*n-2*i;
            spaces(space);
            stars(star);
            System.out.println();
        }
        }
        void stars(int n)
        {
            for(int i=0;i<n;i++)
            {
                System.out.print("*");
            }
        }
        void spaces(int n)
        {
            for(int j=0;j<n;j++)
            {
                System.out.print(" ");
            }
        }
        
    }
    
    
    
