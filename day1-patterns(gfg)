class Solution {

    void printTriangle(int n) {
        for(int i=1;i<=2*n-1;i++)
        {
            int stars=i<=n?i:2*n-i;
            star(stars);
            int spaces=i<=n?2*(n-i):2*(i-n);
            space(spaces);
            star(stars);
        System.out.println();
    }}
        
        void star(int n)
        {
            for(int i=0;i<n;i++)
            {
                System.out.print("*");
            }
        }
        void space(int n)
        {
            for(int i=0;i<n;i++)
            {
                System.out.print(" ");
            }
        }
        
    }


