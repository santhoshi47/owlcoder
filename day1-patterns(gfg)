class Solution {

    void printTriangle(int n) {
      for(int i=1;i<=n;i++)
      {
          for(int j=1;j<=i;j++)
          {
             System.out.print(1-(i+j)%2+" "); 
          }
          System.out.println();
      }
    }
}

 
